<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforg.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ml.battlecall.combineKey.MyStudent" table="mystudent">

        <composite-id>
            <key-property name="name" column="table_name" type="string"></key-property>
            <key-property name="cardId" column="table_cardId" type="string"></key-property>
        </composite-id>
        <property name="age" column="table_age" type="int">
        </property>

    </class>
</hibernate-mapping>